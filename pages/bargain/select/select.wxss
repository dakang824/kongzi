.headTopNav {
  height: 200rpx;
  z-index: 999;
  position: fixed;
  top: 0;
  left: 0;
  right: 0;
}

.vCenter van-icon {
  display: flex;
  align-items: center;
}

.location-o {
  display: flex;
  align-items: center;
  margin-right: 8rpx;
}

.union_tag {
  margin: 8rpx 0 20rpx 0;
}

.union {
  margin-right: 20rpx;
}

.van-tag {
  padding: 4rpx 13rpx;
  background: #f8f8f8;
  border-radius: 8rpx;
  font-size: 20rpx;
  color: #666;
}

.bar {
  position: relative;
  margin-top: 200rpx;
  overflow: hidden;
}

.van-tag {
  margin-right: 20rpx;
  text-align: center;
  max-height: 30rpx;
}

van-tag {
  display: flex;
  align-items: center;
}

.loading {
  text-align: center;
  font-size: 28rpx;
  color: #999;
  padding: 30rpx 0;
}

.noData {
  font-size: 28rpx;
  color: #999;
  margin-top: 150rpx;
  width: 750rpx;
  text-align: center;
}

.address {
  margin: 0 30rpx 20rpx;
  padding-top: 20rpx;
  font-size: 34rpx;
  color: #fff;
}

.van-transition {
  top: 130rpx !important;
}

.price {
  position: absolute;
  right: 0;
  top: 0;
}

.list_rt {
  width: 471rpx;
  position: relative;
}

.van_tabs scroll-view, .van-tabs--line .van-tabs__wrap {
  width: 85%;
}

.screen {
  font-size: 24rpx;
  color: #fff;
  padding: 0 15rpx 0 15rpx;
  z-index: 9;
}

.van-search__field {
  display: flex;
}

.triangle-up {
  width: 0;
  height: 0;
  border-left: 16rpx solid transparent;
  border-right: 16rpx solid transparent;
  border-bottom: 16rpx solid #fff;
  position: absolute;
  bottom: 0;
  right: 60rpx;
}

.van-cell {
  width: 600rpx !important;
  padding: 15rpx 20rpx !important;
}

.van-field__input {
  width: 86% !important;
}

.van-field_clear-root {
  display: none !important;
}

.screen image {
  width: 16px;
  height: 14px;
  vertical-align: middle;
  position: relative;
  top: -2px;
  left: -3px;
}

.list .li {
  padding: 25rpx 0;
  margin: 0 30rpx;
  border-bottom: 1rpx solid #f3f3f3;
  display: flex;
  align-content: center;
}

.list_lf {
  position: relative;
  border-radius: 5px;
}

.list_lf .label {
  position: absolute;
  top: 20rpx;
  left: -8rpx;
  height: 40rpx;
  font-size: 20rpx;
  color: #fff;
  text-align: center;
  line-height: 40rpx;
  background: linear-gradient(90deg, rgba(98, 134, 253, 1), rgba(77, 180, 252, 1));
  border-top-right-radius: 6rpx;
  border-bottom-right-radius: 6rpx;
  border-bottom-left-radius: 6rpx;
  padding: 0 6rpx;
}

.list_lf .sanjiao {
  width: 0;
  height: 0;
  border-width: 8rpx 8rpx 0 0;
  border-style: solid;
  border-color: transparent #086ed3 transparent;
  position: absolute;
  top: 13rpx;
  left: -8rpx;
}

.list_lf .label_bg {
  position: absolute;
  top: 20rpx;
  left: -8rpx;
  border-radius: 0 6rpx 6rpx 0;
}

.list_lf .label .arg {
  width: 0;
  height: 0;
  border-width: 8rpx 8rpx 0 0;
  border-style: solid;
  border-color: transparent #086ed3 transparent;
  position: absolute;
  top: -7rpx;
  left: 1rpx;
}

.list_lf image {
  width: 200rpx;
  height: 200rpx;
  border-radius: 5px;
  margin-right: 24rpx;
  vertical-align: middle;
}

.list_rt .actName {
  font-size: 32rpx;
  margin-bottom: 12rpx;
}

.list_rt .status {
  font-size: 24rpx;
  margin-top: 20rpx;
}

.list_rt .status .startImg {
  width: 18rpx;
  height: 20rpx;
}

.list_rt .status .doingImg {
  width: 20rpx;
  height: 24rpx;
}

.list_rt .status .lightningImg {
  width: 18rpx;
  height: 26rpx;
}

.list_rt .status image {
  margin-right: 10rpx;
  vertical-align: middle;
}

.list_rt .status .star {
  color: #4da5fc;
}

.list_rt .status .doing {
  color: #ffa740;
}

.list_rt .status .doing {
  color: #fb2323;
}

.list_rt .act_time {
  font-size: 24rpx;
  color: rgba(51, 51, 51, 1);
  margin-top: 18rpx;
}

.min-width {
  padding-bottom: 7rpx;
  padding-top: 5rpx;
}

.list_rt .list_fter {
  display: flex;
  justify-content: space-between;
  font-size: 22rpx;
  margin-top: 20rpx;
  color: #999;
  align-items: center;
}

.list_rt .list_fter text:first-child {
  border-radius: 100px;
  color: #fff;
  font-size: 20rpx;
  background: #63b1fe;
  width: 30rpx;
  height: 30rpx;
  display: inline-block;
  text-align: center;
  margin-right: 10rpx;
}

.list_rt .list_fter text:last-child {
  color: #999;
}

.act_time text {
  width: 40rpx;
  height: 40rpx;
  border: 1px solid rgba(251, 35, 35, 1);
  border-radius: 6rpx;
  display: inline-block;
  line-height: 40rpx;
  text-align: center;
  margin: 0 10rpx;
  font-size: 24rpx;
  color: #fb2323;
  font-weight: bold;
}

.van-overlay {
  top: 200rpx;
}

.popup {
  top: 200rpx !important;
}

.popup .title {
  font-size: 28rpx;
  margin: 70rpx 0 49rpx 0;
  font-weight: bold;
}

.custom-button {
  width: 30rpx;
  height: 30rpx;
  line-height: 30rpx;
  background: #ff2c56;
  border-radius: 50px;
  text-align: center;
  position: relative;
}

.custom-button .white {
  width: 15rpx;
  height: 15rpx;
  border-radius: 50px;
  background: #fff;
  position: absolute;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  margin: auto;
  display: block;
}

.custom-button .txt {
  position: absolute;
  left: -32rpx;
  bottom: -39rpx;
  font-size: 24rpx;
  width: 100rpx;
}

.block .li {
  border: 1px solid #ccc;
  font-size: 24rpx;
  float: left;
  margin-right: 27rpx;
  border-radius: 25px;
  padding: 15rpx 28rpx;
  margin-bottom: 35rpx;
}

.block .li.active {
  border: 1px solid #ff2c56;
  color: #ff2c56;
}

.fiexed {
  position: absolute;
  bottom: 0;
  left: 0;
  right: 0;
  display: flex;
}

.fiexed view {
  width: 50%;
  height: 80rpx;
  line-height: 80rpx;
  text-align: center;
}

.fiexed view:first-child {
  background: #f6f6f6;
  font-size: 32rpx;
  color: #ccc;
}

.fiexed view:last-child {
  background: linear-gradient(90deg, rgba(255, 90, 144, 1), rgba(255, 44, 86, 1));
  font-size: 32rpx;
  color: #fff;
}

.address van-icon {
  display: flex;
  align-items: center;
}

.address_img {
  width: 30rpx !important;
  margin-left: 10rpx;
}

.address .t {
  width: 70%;
}

.address {
  margin: 0 30rpx 20rpx;
  padding-top: 20rpx;
  font-size: 34rpx;
  color: #fff;
  display: flex;
  justify-content: space-between;
}

.red {
  color: #ff5489;
}

.min_height {
  min-height: 36rpx;
}
