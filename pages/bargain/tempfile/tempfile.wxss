page{
  background: #F8F8F8;
}
.start .score{
  margin-left: 4px;
}
.start{
  margin: 23rpx 0 0;
  font-size:24rpx;
}
.headTopNav {
  height: 200rpx;
  z-index: 999999;
}

::-webkit-scrollbar {
  display: none;
}

.hide {
  overflow: hidden;
}

.data_box {
  min-height: calc(100vh - 100rpx);
}

.list_lf {
  position: relative;
  border-radius: 5px;
}

.address van-icon {
  display: flex;
  align-items: center;
}

.text_hidden {
  display: inline-block;
}

.address_img {
  width: 30rpx !important;
  margin-left: 10rpx;
}

.list_lf .label {
  position: absolute;
  top: 20rpx;
  left: -7rpx;
  height: 40rpx;
  font-size: 20rpx;
  color: #fff;
  text-align: center;
  line-height: 40rpx;
  background: linear-gradient(90deg, rgba(98, 134, 253, 1), rgba(77, 180, 252, 1));
  border-top-right-radius: 6rpx;
  border-bottom-right-radius: 6rpx;
  border-bottom-left-radius: 6rpx;
  padding: 0 6rpx;
}

.list_lf .label .arg {
  width: 0;
  height: 0;
  border-width: 8rpx 8rpx 0 0;
  border-style: solid;
  border-color: transparent #086ed3 transparent;
  position: absolute;
  top: -7rpx;
  left: 1rpx;
}

.list_lf image {
  width:157rpx;
  height: 157rpx;
  border-radius: 10rpx;
  margin-right: 24rpx;
  vertical-align: middle;
}

.fixed {
  position: fixed;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
}

.loading {
  text-align: center;
  font-size: 28rpx;
  color: #999;
  padding: 30rpx 0;
}

.noData {
  font-size: 28rpx;
  color: #999;
  margin-top: 150rpx;
  width: 750rpx;
  text-align: center;
}

.address .t {
  width: 70%;
}

.address {
  margin: 0 30rpx 20rpx;
  padding-top: 20rpx;
  font-size: 34rpx;
  color: #fff;
  display: flex;
  justify-content: space-between;
}

/* .address van-icon{
  display: flex;
} */

.van-transition {
  top: 200rpx !important;
}

.list_rt {
  width: 471rpx;
}

.van_tabs scroll-view, .van-tabs--line .van-tabs__wrap {
  width: 85%;
}

.screen {
  font-size: 24rpx;
  color: #fff;
  padding: 0 15rpx 0 15rpx;
  z-index: 9;
}

.van-search__field {
  display: flex;
}

.triangle-up {
  width: 0;
  height: 0;
  border-left: 16rpx solid transparent;
  border-right: 16rpx solid transparent;
  border-bottom: 16rpx solid #fff;
  position: absolute;
  bottom: 0;
  right: 60rpx;
}

.van-cell {
  width: 600rpx !important;
  padding: 15rpx 20rpx !important;
}

.van-field__input {
  width: 86% !important;
}

.van-field_clear-root {
  display: none !important;
}

.screen image {
  width: 16px;
  height: 14px;
  vertical-align: middle;
  position: relative;
  top: -2px;
  left: -3px;
}

.content {
  position: relative;
}
.box:first-child{
  margin-top: 30rpx;
}
.box {
  width: 655rpx;
  padding: 28rpx 25rpx;
  margin: 0 auto 30rpx;
  background: #fff;
  border-radius:20rpx;
}

.box_left {
  width: 200rpx;
  height: 200rpx;
  float: left;
  border-radius: 10rpx;
  vertical-align: middle;
}

.box_right {
  float: left;
  width: 467rpx;
  display: flex;
  flex-direction: column;
}

.name {
  font-size: 31rpx;
  line-height: 31rpx;
  font-family: PingFang-SC-Bold;
  font-weight: bold;
  color: rgba(51, 51, 51, 1);
}

.num {
  font-size: 24rpx;
  font-family: PingFang-SC-Medium;
  font-weight: 500;
  color: rgba(51, 51, 51, 1);
}

.footer {
  margin-top: 20rpx;
}

.profile {
  width: 453rpx;
  font-size: 24rpx;
  font-family: PingFang-SC-Regular;
  font-weight: 400;
  color: rgba(153, 153, 153, 1);
  line-height: 36rpx;
  display: block;
  margin-top: 13rpx;
}

.select1, .select2, .icon_address, .word {
  float: left;
}

.select1:nth-child(4n) {
  margin-right: 0;
}

.select1 {
  height: 30rpx;
  background: rgba(255, 255, 255, 1);
  border: 1px solid rgba(77, 165, 252, 1);
  border-radius: 10rpx;
  font-size: 20rpx;
  font-family: PingFang-SC-Regular;
  font-weight: 400;
  color: rgba(77, 165, 252, 1);
  text-align: center;
  line-height: 30rpx;
  margin-right: 10rpx;
  margin-bottom: 10rpx;
  padding: 3rpx 13rpx;
}

.select2 {
  height: 30rpx;
  background: rgba(255, 255, 255, 1);
  border: 1px solid rgba(77, 165, 252, 1);
  border-radius: 10rpx;
  font-size: 20rpx;
  font-family: PingFang-SC-Regular;
  font-weight: 400;
  color: rgba(77, 165, 252, 1);
  text-align: center;
  line-height: 30rpx;
  padding: 0 10rpx;
}

.select3 {
  overflow: hidden;
  min-width: 133rpx;
  display: flex;
  margin-top: 10rpx;
}

.icon_address {
  max-width: 14rpx;
  min-width: 14rpx;
  height: 18rpx;
  margin-top: 8rpx;
}

.word {
  font-size: 22rpx;
  font-family: PingFang-SC-Regular;
  font-weight: 400;
  color: rgba(51, 51, 51, 1);
  opacity: 0.75;
  margin-left: 5rpx;
}

.act_time text {
  width: 40rpx;
  height: 40rpx;
  border: 1px solid rgba(251, 35, 35, 1);
  border-radius: 6rpx;
  display: inline-block;
  line-height: 40rpx;
  text-align: center;
  margin: 0 10rpx;
  font-size: 24rpx;
  color: #fb2323;
  font-weight: bold;
}

.van-overlay {
  top: 350rpx;
}

.popup {
  top: 200rpx !important;
}

.box-t {
  font-size: 24rpx;
  color: #666;
  margin-top: 10rpx;
}

.box-t .t {
  background: #f8f8f8;
  padding: 1px 7px;
  margin-right: 20rpx;
  border-radius: 8rpx;
}

.popup .title {
  font-size: 28rpx;
  margin: 70rpx 0 49rpx 0;
  font-weight: bold;
}

.custom-button {
  width: 30rpx;
  height: 30rpx;
  line-height: 30rpx;
  background: #ff2c56;
  border-radius: 50px;
  text-align: center;
  position: relative;
}

.custom-button .white {
  width: 15rpx;
  height: 15rpx;
  border-radius: 50px;
  background: #fff;
  position: absolute;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  margin: auto;
  display: block;
}

.custom-button .txt {
  position: absolute;
  left: -32rpx;
  bottom: -39rpx;
  font-size: 24rpx;
  width: 100rpx;
}

.block .li {
  width: 155rpx;
  border: 1px solid #ccc;
  font-size: 24rpx;
  float: left;
  margin-right: 12rpx;
  border-radius: 25px;
  padding: 15rpx 0;
  text-align: center;
  margin-bottom: 35rpx;
}

.van-tab--active {
  color: #ff2c56 !important;
}

.van-tabs__line {
  width: 30rpx !important;
  left: 10% !important;
}

.block .li.active {
  border: 1px solid #ff2c56;
  color: #ff2c56;
}

.van-tabs__wrap {
  width: auto !important;
}

.fiexed {
  position: absolute;
  bottom: 0;
  left: 0;
  right: 0;
  display: flex;
}

.fiexed view {
  width: 50%;
  height: 80rpx;
  line-height: 80rpx;
  text-align: center;
}

.fiexed view:first-child {
  background: #f6f6f6;
  font-size: 32rpx;
  color: #ccc;
}

.fiexed view:last-child {
  background: linear-gradient(90deg, rgba(255, 90, 144, 1), rgba(255, 44, 86, 1));
  font-size: 32rpx;
  color: #fff;
}

.isFixed .van-tab {
  background: #fa2335;
  color: #fff;
}

.fixed {
  position: fixed !important;
  top: 0;
  left: 0;
  right: 0;
}

.isFixed .van-tab--active {
  color: #fff !important;
}
.box van-tag{
  margin-right: 17rpx;
}