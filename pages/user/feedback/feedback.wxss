.top_tip{
  margin: 30rpx;
  font-size: 26rpx;
  margin-bottom: 50rpx;
}
.top_tip image {
  width: 88rpx;
  height: 88rpx;
}
.top_tip .c{
  text-align: center;
  padding: 10rpx 0 25rpx;
}
.top_tip text{
  text-indent: 45rpx;
  display: block;
  line-height: 40rpx;
  color: #333;
}
.txt {
  width: 630rpx;
  height: 300rpx;
  border: 1rpx solid rgba(238, 238, 238, 1);
  border-radius: 20rpx;
  margin: 30rpx auto;
  padding: 30rpx;
  font-size: 28rpx;
}

.total {
  font-size: 28rpx;
  color: #ccc;
  position: absolute;
  right: 60rpx;
  bottom: 20rpx;
  z-index: 9;
}

.f {
  position: relative;
}

.place {
  font-size: 28rpx;
  color: #ccc;
}

.btn {
  height: 150rpx;
  padding-top: 50rpx;
  position: fixed;
  bottom: 0;
  left: 0;
  right: 0;
  margin: auto;
}

.tip {
  font-size: 24rpx;
  color: #ccc;
  padding-left: 30rpx;
}

.home-btn {
  width: 500rpx;
  height: 82rpx;
  border-radius: 42rpx;
  background: #eee;
  font-size: 32rpx;
  line-height: 82rpx;
  color: #fff !important;
  text-align: center;
  margin: auto;
  border-color: transparent !important;
}

.home-btn.active {
  background: linear-gradient(to right, #3d7fda, #4da5fc);
  box-shadow: 3rpx 3rpx 5rpx #3d7fda;
}
