  width: 290rpx;
  font-size: 22rpx;
  font-weight: 500;
  color: rgba(255, 255, 255, 1);
  line-height: 40rpx;
}

.get_tickets .detail .right {
  margin-left: 148rpx;
  align-items: center;
}

.get_tickets .detail .right .rightbox {
  align-items: baseline;
}

.get_tickets .detail .right .num {
  font-size: 44rpx;
  font-family: STXihei;
  font-weight: bold;
  color: rgba(255, 255, 255, 1);
}

.get_tickets .detail .right .unit {
  font-size: 24rpx;
  font-weight: 500;
  color: rgba(255, 255, 255, 1);
  margin-left: 12rpx;
}

.get_tickets .getbtn {
  width: 500rpx;
  height: 80rpx;
  background: linear-gradient(90deg, rgba(255, 90, 144, 1), rgba(255, 44, 86, 1));
  border-radius: 40rpx;
  margin: 50rpx auto 0;
  text-align: center;
  font-size: 32rpx;
  font-weight: 500;
