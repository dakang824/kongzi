.get_tickets {
  width: 650rpx;
  height: 670rpx;
  background: rgba(255, 255, 255, 1);
  border-radius: 20px;
  position: fixed;
  z-index: 20;
  left: 50%;
  top: 200rpx;
  transform: translate(-50%);
}

.get_tickets .img {
  width: 353rpx;
  height: 279rpx;
  margin: -130rpx auto 0;
}

.get_tickets .send {
  font-size: 44rpx;
  font-weight: bold;
  color: rgba(17, 17, 17, 1);
  line-height: 56rpx;
  text-align: center;
}

.get_tickets .way {
  font-size: 24rpx;
  color: rgba(102, 102, 102, 1);
  line-height: 56rpx;
  text-align: center;
}

.get_tickets .detail {
  width: 590rpx;
  height: 200rpx;
  background: linear-gradient(-30deg, rgba(254, 77, 88, 1), rgba(253, 79, 118, 1));
  box-shadow: 0px 0px 30rpx 0px rgba(153, 153, 153, 0.2);
  border-radius: 10rpx;
  margin: 35rpx auto 0;
  position: relative;
}

.get_tickets .detail .topicon {
  width: 40rpx;
  height: 40rpx;
  border-radius: 0 0 50% 50%;
  background: rgba(255, 255, 255, 1);
  position: absolute;
  top: -20rpx;
  right: 160rpx;
}

.get_tickets .detail .btnicon {
  width: 40rpx;
  height: 40rpx;
  border-radius: 50% 50% 0 0;
  background: rgba(255, 255, 255, 1);
  position: absolute;
  bottom: -20rpx;
  right: 160rpx;
}

.get_tickets .detail .midicon {
  /* width:1rpx; */
  height: 129rpx;
  border: 1rpx dashed rgba(255, 255, 255, 1);
  position: absolute;
  top: 50%;
  right: 180rpx;
  transform: translate(0, -50%);
}

.get_tickets .detail .left {
  flex-direction: column;
  justify-content: center;
  margin-left: 30rpx;
}

.get_tickets .detail .left .title {
  font-size: 38rpx;
  font-weight: 800;
  color: rgba(255, 255, 255, 1);
}

