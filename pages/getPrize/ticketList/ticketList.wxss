
.btmbox{
    margin-top:30rpx;
}
.btmbox .itembox .leftbox{
    margin:30rpx 0 0 30rpx;
    width:180rpx;
    height:200rpx;
    background:rgba(255,255,255,1);
    border:2rpx dashed rgba(255,202,213,1);
    border-radius:20rpx 0 0 20rpx; 
    border-right:none;
    position: relative;
    display: flex;
    flex-direction: column;
    justify-content: center;
    align-items: center;
}
.btmbox .itembox .leftbox .typebox{
    text-align: center;
}
.btmbox .itembox .leftbox .typebox .type{
    font-size:32rpx;
    font-weight:bold;
    color:rgba(51,51,51,1);
    line-height:44rpx;
}
.btmbox .itembox .leftbox .typebox .getbtn{
    width:120rpx;
    height:40rpx;
    background:rgba(255, 44, 86, 1);
    border-radius:20rpx;
    font-size:22rpx;
    color:rgba(254, 254, 254, 1);
    line-height:40rpx;
    text-align: center;
    margin-top:30rpx;
}
.btmbox .itembox .leftbox .typebox .hasget{
    background:rgba(245,245,245,1);
    color:rgba(153,153,153,1);
}
.btmbox .itembox .leftbox .top_icon{
    position: absolute;
    top:-4rpx;
    right:-30rpx;
    width: 30rpx;
    height: 20rpx;
    border-radius:0 0 50% 50%/0 0 100% 100%;
    border:2rpx dashed rgba(255,202,213,1);
    border-top:none;
}
.btmbox .itembox .leftbox .btm_icon{
    position: absolute;
    bottom:-4rpx;
    right:-30rpx;
    width: 30rpx;
    height: 20rpx;
    border-radius:50% 50% 0 0/100% 100% 0 0;
    border:2rpx dashed rgba(255,202,213,1);
    border-bottom:none;
}
.btmbox .itembox .leftbox .middle_icon{
    position:absolute;
    width:2rpx;
    height:120rpx;
    border-right:2rpx dashed rgba(255,202,213,1);
    right:-15rpx;
    top:50%;
    transform: translate(0,-50%)
}
.btmbox .itembox .rightbox{
    margin:30rpx 20rpx 0rpx 0rpx;
    width:470rpx;
    height:200rpx;
    background:rgba(255,255,255,1);
    border:2rpx dashed rgba(255,202,213,1);
    border-radius:0 20rpx 20rpx 0; 
    border-left:none;
    margin-left: 30rpx;
    display: flex;
    flex-direction: column;
    justify-content: center;
    /* align-items: center; */
}
.btmbox .itembox .rightbox .com{
    font-size:22rpx;
    font-weight:500;
    color:rgba(153,153,153,1);
    line-height:44rpx;
    margin-left: 25rpx;
}
.btmbox .itembox .rightbox .time1{
    align-items: baseline;
}
.btmbox .itembox .rightbox .time1 .notice text{
    color:#FF2C56;
}
.btmbox .itembox .rightbox .time1 .tip{
    width:100rpx;
    height:26rpx;
    background:rgba(255,44,86,1);
    border-radius:13rpx;
    font-size:18rpx;
    color:rgba(255,255,255,1);
    line-height:26rpx;
    text-align: center;
    margin-left:15rpx;
}


