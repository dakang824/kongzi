.flex {
  display: flex;
}

.header {
  margin: 30rpx auto 0;
  width: 690rpx;
  height: 250rpx;
  background: rgba(255, 255, 255, 1);
  box-shadow: 0px 0px 30rpx 0px rgba(153, 153, 153, 0.2);
  border-radius: 20rpx;
  display: flex;
  flex-direction: column;
}

.header .top {
  margin: 0 auto;
  width: 650rpx;
  height: 104rpx;
  background: rgba(255, 255, 255, 1);
  border-bottom: 1rpx solid rgba(238, 238, 238, 1);
  align-items: center;
}

.header .top .tip {
  font-size: 24rpx;
  font-weight: 500;
  color: rgba(17, 17, 17, 1);
  line-height: 54rpx;
  margin-left: 20rpx;
}

.header .top .img image {
  width: 44rpx;
  height: 44rpx;
}

.header .btm {
  flex: 1;
  align-items: center;
  margin-left: 20rpx;
}

.header .btm .left image {
  width: 80rpx;
  height: 80rpx;
}

.header .btm .right {
  margin-left: 20rpx;
}

.header .btm .right .name {
  font-size: 28rpx;
  font-weight: bold;
  color: rgba(17, 17, 17, 1);
}

.header .btm .right .note {
  font-size: 24rpx;
  color: rgba(153, 153, 153, 1);
}

.btmbox {
  margin-top: 30rpx;
}

.btmbox .itembox .leftbox {
  margin: 30rpx 0 0 30rpx;
  width: 180rpx;
  height: 200rpx;
  background: rgba(255, 255, 255, 1);
  border: 2rpx dashed rgba(255, 202, 213, 1);
  border-radius: 20rpx 0 0 20rpx;
  border-right: none;
  position: relative;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
}

.btmbox .itembox .leftbox .typebox {
  text-align: center;
}

.btmbox .itembox .leftbox .typebox .type {
  font-size: 32rpx;
  font-weight: bold;
  color: rgba(51, 51, 51, 1);
  line-height: 44rpx;
}

.btmbox .itembox .leftbox .typebox .getbtn {
  line-height: 40rpx;
  text-align: center;
  margin-top: 30rpx;
  width: 120rpx;
  height: 40rpx;
  background: rgba(255, 44, 86, 1);
  border-radius: 20rpx;
  font-size: 22rpx;
  color: rgba(254, 254, 254, 1);
}

.btmbox .itembox .leftbox .typebox .hasget {
  background: rgba(245, 245, 245, 1);
  color: rgba(153, 153, 153, 1);
}

.btmbox .itembox .leftbox .top_icon {
  position: absolute;
  top: -4rpx;
  right: -30rpx;
  width: 30rpx;
  height: 20rpx;
  border-radius: 0 0 50% 50%/0 0 100% 100%;
  border: 2rpx dashed rgba(255, 202, 213, 1);
  border-top: none;
}

.btmbox .itembox .leftbox .btm_icon {
  position: absolute;
  bottom: -4rpx;
  right: -30rpx;
  width: 30rpx;
  height: 20rpx;
  border-radius: 50% 50% 0 0/100% 100% 0 0;
  border: 2rpx dashed rgba(255, 202, 213, 1);
  border-bottom: none;
}

.btmbox .itembox .leftbox .middle_icon {
  position: absolute;
  width: 2rpx;
  height: 120rpx;
  border-right: 2rpx dashed rgba(255, 202, 213, 1);
  right: -15rpx;
  top: 50%;
  transform: translate(0, -50%);
}

.btmbox .itembox .rightbox {
  margin: 30rpx 20rpx 0rpx 0rpx;
  width: 470rpx;
  height: 200rpx;
  background: rgba(255, 255, 255, 1);
  border: 2rpx dashed rgba(255, 202, 213, 1);
  border-radius: 0 20rpx 20rpx 0;
  border-left: none;
  margin-left: 30rpx;
  display: flex;
  flex-direction: column;
  justify-content: center;
  /* align-items: center; */
}

.btmbox .itembox .rightbox .com {
  font-size: 22rpx;
  font-weight: 500;
  color: rgba(153, 153, 153, 1);
  line-height: 44rpx;
  margin-left: 25rpx;
}

.btmbox .itembox .rightbox .time1 {
  align-items: baseline;
}

.btmbox .itembox .rightbox .time1 .notice text {
  color: #ff2c56;
}

.btmbox .itembox .rightbox .time1 .tip {
  width: 100rpx;
  height: 26rpx;
  background: rgba(255, 44, 86, 1);
  border-radius: 13rpx;
  font-size: 18rpx;
  color: rgba(255, 255, 255, 1);
  line-height: 26rpx;
  text-align: center;
  margin-left: 15rpx;
}
.mask {
  width: 750rpx;
  height: 100vh;
  background: rgba(0, 0, 0, 1);
  opacity: 0.7;
  position: fixed;
  z-index: 10;
  left: 0;
  top: 0;
}
.prizebox{
  width:630rpx;
  height:630rpx;
  background:linear-gradient(55deg,rgba(59,42,209,1),rgba(255,128,219,1));
  box-shadow:0px 34rpx 59rpx 0px rgba(0, 0, 0, 0.26);
  border-radius:30rpx;
  position: fixed;
  z-index: 20;
  left: 50%;
  top: 50%;
  transform: translate(-50%,-50%);
}
.prizebox .titlebox{
  width:500rpx;
  height:150rpx;
  background:rgba(255,255,255,1);
  box-shadow:0px 10rpx 50rpx 0px rgba(132,44,255,0.5);
  text-align: center;
  margin: 10rpx auto 0;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  font-size:32rpx;
  font-family:PingFang SC;
  font-weight:bold;
  color:#3B2AD1;
  line-height:50rpx;
  margin-top: 20rpx;
}
.prizebox .row1{
  font-size:24rpx;
  font-family:PingFang SC;
  font-weight:500;
  color:rgba(255,255,255,1);
  text-align: center;
  margin-top:-60rpx;

}
.prizebox .img{
 text-align: center;
 margin-top: -180rpx;
}
.prizebox .img image{
  width:523rpx;
  height: 443rpx;
}
.prizebox .row2{
  font-size:26rpx;
  font-family:PingFang SC;
  font-weight:500;
  color:rgba(255,255,255,1);
  line-height:44rpx;
  text-align:center;
  margin-top: 27rpx;
}
.prizebox .row3{
  width:372rpx;
  height:63rpx;
  background:rgba(255,255,255,1);
  box-shadow:0px 11rpx 21rpx 0px rgba(0, 0, 0, 0.26);
  border-radius:32rpx;
  font-size:32rpx;
  font-family:PingFang SC;
  font-weight:bold;
