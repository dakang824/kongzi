
.flex{
  display: flex;
}
.nodatabox .img {
  text-align: center;
  margin-top: 400rpx;
}

.nodatabox .tip {
  text-align: center;
  font-size: 28rpx;
  font-family: PingFang SC;
  font-weight: 400;
  color: rgba(51, 51, 51, 1);
  line-height: 54rpx;
  margin-top: 49rpx;
}

.nomore {
  font-size: 28rpx;
  font-family: PingFang SC;
  font-weight: 400;
  color: rgba(51, 51, 51, 1);
  line-height: 44rpx;
  text-align: center;
  margin-top: 20rpx;
}

.join {
  width: 160rpx;
  height: 60rpx;
  background: linear-gradient(90deg, rgba(255, 90, 144, 1), rgba(255, 44, 86, 1));
  border-radius: 30rpx;
  font-size: 28rpx;
  font-family: PingFang SC;
  font-weight: 500;
  color: rgba(255, 255, 255, 1);
  line-height: 60rpx !important;
  text-align: center;
}

.active_join {
  background: rgba(229, 229, 229, 1);
  color: #fff;
}

.serachbox {
  position: fixed;
  top: 0rpx;
  left: 0rpx;
  width: 100%;
  z-index: 11;
  background: #fff;
  height: 100rpx;
}

.serachbox .item {
  width: 690rpx;
  justify-content: space-between;
  margin: 20rpx auto;
}

.serachbox .item .left {
  width: 690rpx;
  height: 70rpx;
  background: rgba(245, 245, 245, 1);
  border-radius: 30rpx;
}

.serachbox .item .left input {
  font-size: 26rpx;
  font-family: PingFang SC;
  font-weight: 400;
  color: rgba(153, 153, 153, 1);
  height: 100%;
  line-height: 100%;
}

.serachbox .item .left .v {
  font-size: 26rpx;
  font-family: PingFang SC;
  font-weight: 400;
  color: rgba(153, 153, 153, 1);
  text-align: center;
  height: 100%;
  line-height: 100%;
  width: 690rpx;
  height: 70rpx;
  background: rgba(245, 245, 245, 1) !important;
  border-radius: 30rpx;
}

.v .van-cell {
  background: rgba(245, 245, 245, 1) !important;
}

.serachbox .item .right {
  width: 160rpx;
  height: 60rpx;
  border-radius: 30rpx;
  justify-content: center;
  align-items: center;
  font-size: 26rpx;
  font-family: PingFang SC;
  font-weight: 400;
  color: rgba(17, 17, 17, 1);
  position: relative;
}

.serachbox .item .right .allbox {
  justify-content: center;
  align-items: center;
}

.serachbox .item .right .allbox .all {
  margin-left: 7rpx;
}

.itembox {
  /* margin-top: 100rpx; */
  position: fixed;
  top: 100rpx;
  left: 0rpx;
  height: 84rpx;
  z-index: 11;
  width: 100%;
  background: #fff;
}

.itembox .item {
  height: 84rpx;
  border-bottom: 1rpx solid #eee;
  justify-content: space-around;
  align-items: center;
}

.itembox .item .default {
  font-size: 26rpx;
  font-family: PingFang SC;
  font-weight: 500;
  color: rgba(153, 153, 153, 1);
}

.itembox .item .default .value {
  display: flex;
  flex-direction: column;
  justify-content: center;
  margin-left: 7rpx;
}

.popup {
  top: 100rpx !important;
}

.reset {
  background: #f6f6f6;
  font-size: 32rpx;
  color: #ccc;
  width: 50%;
  height: 80rpx;
  line-height: 80rpx;
  text-align: center;
}

.sure {
  background: linear-gradient(90deg, rgba(255, 90, 144, 1), rgba(255, 44, 86, 1));
  font-size: 32rpx;
  color: #fff;
  width: 50%;
  height: 80rpx;
  line-height: 80rpx;
  text-align: center;
}

.type {
  padding: 0 30rpx;
}

.type .top {
  font-size: 28rpx;
  font-family: PingFang SC;
  font-weight: bold;
  color: rgba(51, 51, 51, 1);
  margin-top: 40rpx;
}

.type .btn {
  flex-wrap: wrap;
}

.type .btn .item {
  background: rgba(255, 255, 255, 1);
  border: 1rpx solid rgba(204, 204, 204, 1);
  border-radius: 27rpx;
  padding: 10rpx 30rpx;
  font-size: 24rpx;
  font-family: PingFang SC;
  font-weight: 500;
  color: rgba(51, 51, 51, 1);
  margin-right: 30rpx;
  margin-top: 30rpx;
}
.type .btn .item:nth-child(5n){
  margin-right: 0;
}
.type .btn .active {
  color: #ff2c56;
  border: 1rpx solid #ff2c56;
}

.pageFiexed {
  overflow: hidden;
  height: 100vh;
  width: 100vw;
  position: fixed;
  top: 0;
  left: 0;
  bottom: 0;
  right: 0;
}
.shopitem{
  margin-top: 184rpx;
  padding: 0 30rpx;
}
.shopitem .item{
  height: 280rpx;
  align-items:center;
  border-bottom: 1rpx solid #EEEEEE;
}
.shopitem .item .left{
  width:200rpx;
  height:200rpx;
  border-radius:20rpx;
  background:rgba(245,245,245,1);
  position: relative;
}
.shopitem .item .left .over{
  position: absolute;
  top: 50%;
  left:50%;
  transform: translate(-50%,-50%);
  width:100rpx;
  height:40rpx;
  background:rgba(102,102,102,1);
  border-radius:6rpx;
  line-height: 40rpx;
  text-align: center;
  font-size:24rpx;
  font-family:PingFang SC;
  font-weight:400;
  color:rgba(255,255,255,1);
}
.shopitem .item .left image{
  width:200rpx;
  height:200rpx;
  border-radius:20rpx;
  background:rgba(245,245,245,1);
}
.shopitem .item .right{
  margin-left: 30rpx;
  height: 200rpx;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  flex: 1;
}
.shopitem .item .right .row1{
  font-size:28rpx;
  font-family:PingFang SC;
  font-weight:bold;
  color:rgba(17,17,17,1);
  line-height:44rpx;
}
.shopitem .item .right .row2{
  justify-content: space-between;
  align-items: center;
}
.shopitem .item .right .row2 .price{
  font-size:22rpx;
  font-family:PingFang SC;
  font-weight:bold;
  color:rgba(255,44,86,1);
}
.shopitem .item .right .row2 .count{
  font-size:22rpx;
  font-family:PingFang SC;
  font-weight:400;
  color:rgba(153,153,153,1)
}
.shopitem .item .right .row2 .buy{
  width:120rpx;
  height:50rpx;
  background:linear-gradient(90deg,rgba(255,90,144,1),rgba(255,44,86,1));
  border-radius:25rpx;
  font-size:24rpx;
  font-family:PingFang SC;
  font-weight:400;
  color:rgba(255,255,255,1);
  text-align: center;
  line-height: 50rpx;
}