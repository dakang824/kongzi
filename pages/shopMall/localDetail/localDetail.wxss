.flex{
  display: flex;
}
.cardbox{
  margin-top: 30rpx;
  padding: 0 30rpx;

}
.cardbox .content{
  background:rgba(255,255,255,1);
  border-radius:20rpx;
}

van-cell-group .van-field__input{
  text-align: right;
}
.inputValue .van-cell{
  padding: 10px 0;
}
.cardbox .content .img{
  position: relative;
  width: 690rpx;
  border-radius: 20rpx 20rpx 0rpx 0rpx;
}
.cardbox .content .img .lineup{
  width: 100rpx;
  height: 100rpx;
  position: absolute;
  top: -3rpx;
  left: 0rpx;
}
.cardbox .content .img .over{
  width: 200rpx;
  height: 160rpx;
  position: absolute;
  top: 50%;
  left: 50%;
  transform: translate(-50%,-50%);
}
.cardbox .content .img .time{
  background:rgba(255,44,86,1);
  border-radius:6rpx;
  position: absolute;
  top: 30rpx;
  right: -10rpx;
  font-size:26rpx;
  font-family:PingFang SC;
  color:rgba(255,255,255,1);
  text-align: center;
  padding: 2rpx 10rpx;
  z-index: 8;
}
.cardbox .content .img image{
  border-radius: 20rpx;
  width: 690rpx;
}
.cardbox .content .name{
  padding: 25rpx 0rpx 10rpx 0rpx;
  font-size:28rpx;
  font-family:PingFang SC;
  font-weight:bold;
  color:rgba(17,17,17,1);
  line-height:44rpx;
}
.cardbox .content .overtip{
  width:750rpx;
  height:72rpx;
  background:rgba(204,204,204,1);
  margin-left: -30rpx;
  line-height: 72rpx;
  text-align: center;
  font-size:32rpx;
  font-family:PingFang SC;
  font-weight:500;
  color:rgba(255,255,255,1);
  margin-top: 30rpx;
}
.cardbox .content .line{
  width: 100%;
  height: 1rpx;
  background: #eee;
  margin-top: 20rpx;
}
.cardbox .content .price{
  padding: 0rpx 0rpx 0 0;
  align-items: center;
  justify-content: space-between;
}
.cardbox .content .price .left {
  align-items: baseline;
}
.cardbox .content .price .left .yuan{
  font-size:38rpx;
  font-family:PingFang SC;
  font-weight:bold;
  color:rgba(255,44,86,1);
}
.cardbox .content .price .left .sale{
  font-size:24rpx;
  font-family:PingFang SC;
  font-weight:400;
  color:rgba(153,153,153,1);
  margin-left: 25rpx;
}
.cardbox .content .price .left .sale text{
  text-decoration: line-through;
}
.cardbox .content .price .left .sell{
  font-size:24rpx;
  font-family:PingFang SC;
  font-weight:400;
  color:rgba(153,153,153,1);
}
.cardbox .content .price .left .you{
  font-family:PingFang SC;
  font-weight:400;
  color:rgba(153,153,153,1);
  font-size:24rpx;
}
.cardbox .content .price .right{
  font-size:24rpx;
  font-family:PingFang SC;
  font-weight:400;
  color:rgba(153,153,153,1);
}
.midline1{
  width:750rpx;
  height:20rpx;
  background:rgba(238,238,238,1);
}
.infobox{
  padding: 0 30rpx 50rpx 30rpx;
}
.infobox .title{
  font-size:36rpx;
  font-family:PingFang SC;
  font-weight:bold;
  color:rgba(17,17,17,1);
  line-height:44rpx;
  margin-top: 50rpx;
}
.infobox .b{
  margin-top: 30rpx;
  justify-content: space-between;
  align-items: center;
}
.r .name{
  font-size:28rpx;
  font-family:PingFang SC;
  font-weight:bold;
  color:rgba(17,17,17,1);
  line-height:44rpx;
}
.r .address{
  font-size:24rpx;
  font-family:PingFang SC;
  font-weight:500;
  color:rgba(153,153,153,1);
  line-height:44rpx;
}
.b .right{
  display: flex;
  flex-direction: column;
}
.b .right .distance{
  font-size:24rpx;
  font-family:PingFang SC;
  font-weight:500;
  color:rgba(153,153,153,1);
  line-height:44rpx;
  /* margin-right: 20rpx;
  margin-top:40rpx; */
}
.b .right .consult{
  font-size:24rpx;
  font-family:PingFang SC;
  color:rgba(17,17,17,1);
  line-height:44rpx;
}
.intro{
  padding-bottom: 100rpx;
}
.intro .title{
  font-size:36rpx;
  font-family:PingFang SC;
  font-weight:bold;
  color:rgba(17,17,17,1);
  line-height:44rpx;
  margin-top: 50rpx;
  margin-left: 30rpx;
}
.intro .img{
  text-align: center;
}
.btnbox{
  width: 100%;
  height:98rpx;
  background:rgba(255,128,16,1);
  position: fixed;
  bottom: 0rpx;
  left: 0rpx;
  z-index: 10;
}
.btnbox>view{
  height: 100%;
  width: 50%;
}
.btnbox .left{
  font-size:32rpx;
  font-family:PingFang SC;
  font-weight:500;
  color:rgba(255,255,255,1);
  line-height:98rpx;
  text-align: center;
}
.btnbox .right{
  font-size:32rpx;
  font-family:PingFang SC;
  font-weight:500;
  color:rgba(255,255,255,1);
  line-height:98rpx;
  text-align: center;
  background: #FF2C56;
}


.mask{
  width:750rpx;
  height:100vh;
  background:rgba(0,0,0,1);
  opacity:0.7;
  position: fixed;
  z-index: 10;
  left: 0;
  top:0;
}
.buybox{
  width:750rpx;
  background:rgba(255,255,255,1);
  border-radius:20rpx 20rpx 0 0;
  /* height: 1100rpx; */
}
.buydetail{
  display: flex;
  flex-direction: column;
  width:690rpx;
  align-items: center;
  margin: 0 auto;
}
.buydetail>view{
  width:690rpx;
}
.buydetail .row1{
  font-size:32rpx;
  font-weight:bold;
  color:rgba(17,17,17,1);
  margin: 43rpx 0 20rpx 0;
  text-align: center;
}
.buydetail .row2{
  justify-content: space-between;
  margin-top:30rpx;
}
.buydetail .row2 .key{
  font-size:28rpx;
  font-family:PingFang SC;
  font-weight:500;
  color:rgba(51,51,51,1);
}
.buydetail .row2 .value>view{
  margin: 0 31rpx;
}
.buydetail .boldline{
  width:690rpx;
  height:2rpx;
  background:rgba(238,238,238,1);
  margin-top: 30rpx;
}
.buydetail .row3{
  height: 100rpx;
  border-top: 1rpx solid rgba(238,238,238,1);
  border-bottom: 1rpx solid rgba(238,238,238,1);
  justify-content: space-between;
  align-items: center;
  margin-top: 30rpx;
}
.buydetail .row3 .key{
  font-size:28rpx;
  font-family:PingFang SC;
  font-weight:500;
  color:rgba(153,153,153,1);
}
.buydetail .row3 .value{
  font-size:28rpx;
  font-family:PingFang SC;
  font-weight:500;
  color:rgba(51,51,51,1);
  margin-right: 10rpx;
}
.buydetail .row4{
  height: 180rpx;
  padding-top:60rpx;
  justify-content: space-between;
  box-sizing: border-box;
}
.buydetail .row4 .key{
  font-size:32rpx;
  font-family:PingFang SC;
  font-weight:bold;
  color:rgba(51,51,51,1);
}
.buydetail .row4 .value{
  font-size:44rpx;
  font-family:PingFang SC;
  font-weight:bold;
  color:rgba(51,51,51,1);
  margin-right: 10rpx;
}
.buybox .buybtn{
  width:750rpx;
  height:98rpx;
  background:rgba(255,255,255,1);
  padding: 0 25rpx;
  align-items: center;
  justify-content: space-between;
  box-sizing: border-box;
}
.buybtn {
 /* padding: 50rpx 30rpx; */
 margin-top: 30rpx;
 margin-bottom: 30rpx;
}

.moneybtn .cancel{
  width:330rpx;
  height:82rpx;
  background:rgba(255,255,255,1);
  border:1px solid rgba(255,44,86,1);
  border-radius:41rpx;
  font-size:32rpx;
  font-family:PingFang SC;
  font-weight:500;
  color:rgba(255,44,86,1);
  line-height:82rpx;
  text-align: center;
}

.moneybtn .pay{
  width:330rpx;
  height:82rpx;
  background:linear-gradient(90deg,rgba(255,90,144,1),rgba(255,44,86,1));
  border-radius:41rpx;
  font-size:32rpx;
  font-family:PingFang SC;
  font-weight:500;
  color:rgba(255,255,255,1);
  line-height:82rpx;
  text-align: center;
  margin-left: 30rpx;
}
.shipay{
  padding: 50rpx 30rpx;
  justify-content: space-between;
}
.shipay .title{
  font-size:32rpx;
  font-family:PingFang SC;
  font-weight:bold;
  color:rgba(51,51,51,1);
}
.shipay .money{
  font-size:44rpx;
  font-family:PingFang SC;
  font-weight:bold;
  color:rgba(51,51,51,1);
}
.retail {
  height: 72rpx;
  background: #fff1f1;
  line-height: 72rpx;
  padding: 0 20rpx;
  font-size: 22rpx;
  color: #ff2c56;
  display:flex;
  align-items:center;
  justify-content: space-between;
  box-sizing:border-box;
}

.retail image {
  width: 32rpx;
  height: 32rpx;
  margin-right: 15rpx;
  vertical-align: middle;
}

.swiper {
  position: absolute;
  top: 61rpx;
  z-index: 9;
  width: 100%;
  height: 42rpx;
  line-height: 42rpx;
  border-radius: 21rpx;
  overflow: hidden;
  font-size: 21rpx;
  color: #fff;
}

.swiper image {
  width: 25rpx;
  height: 25rpx;
  vertical-align: middle;
  margin: 0 11rpx 0 7rpx;
}

.swiper_bg{
  background:rgba(125, 0, 0, 0.36);
  border-radius:21rpx;
  padding:0 15rpx 0;
  max-width:672rpx;
  display: flex;
  align-items: center;
  margin-left:75rpx;
}
.gray{
  background: #eee!important;
  width:750rpx!important;
}