.flex{
  display: flex;
}
.nodatabox .img {
  text-align: center;
  margin-top: 220rpx;
}

.nodatabox .tip {
  text-align: center;
  font-size: 28rpx;
  font-weight: 400;
  color: rgba(51, 51, 51, 1);
  line-height: 54rpx;
  margin-top: 49rpx;
}

.nomore {
  font-size: 28rpx;
  font-weight: 400;
  color: rgba(51, 51, 51, 1);
  line-height: 44rpx;
  text-align: center;
  margin-top: 20rpx;
}

.join {
  width: 160rpx;
  height: 60rpx;
  background: linear-gradient(90deg, rgba(255, 90, 144, 1), rgba(255, 44, 86, 1));
  border-radius: 30rpx;
  font-size: 28rpx;
  font-weight: 500;
  color: rgba(255, 255, 255, 1);
  line-height: 60rpx !important;
  text-align: center;
}

.active_join {
  background: rgba(229, 229, 229, 1);
  color: #fff;
}

.serachbox {
  position: fixed;
  top: 0rpx;
  left: 0rpx;
  width: 100%;
  z-index: 11;
  background: #fff;
  height: 100rpx;
}

.serachbox .item {
  width: 690rpx;
  justify-content: space-between;
  margin: 20rpx auto;
}

.serachbox .item .left {
  width: 690rpx;
  height: 70rpx;
  background: rgba(245, 245, 245, 1);
  border-radius: 30rpx;
}

.serachbox .item .left input {
  font-size: 26rpx;
  font-family: PingFang SC;
  font-weight: 400;
  color: rgba(153, 153, 153, 1);
  height: 100%;
  line-height: 100%;
}

.serachbox .item .left .v {
  font-size: 26rpx;
  font-family: PingFang SC;
  font-weight: 400;
  color: rgba(153, 153, 153, 1);
  text-align: center;
  height: 100%;
  line-height: 100%;
  width: 690rpx;
  height: 70rpx;
  background: rgba(245, 245, 245, 1) !important;
  border-radius: 30rpx;
}

.v .van-cell {
  background: rgba(245, 245, 245, 1) !important;
}

.serachbox .item .right {
  width: 160rpx;
  height: 70rpx;
  border-radius: 30rpx;
  justify-content: center;
  align-items: center;
  font-size: 26rpx;
  font-family: PingFang SC;
  font-weight: 400;
  color: rgba(17, 17, 17, 1);
  position: relative;
}

.serachbox .item .right .allbox {
  justify-content: center;
  align-items: center;
}

.serachbox .item .right .allbox .all {
  margin-left: 7rpx;
}

.popup {
  top: 100rpx !important;
}

.reset {
  background: #f6f6f6;
  font-size: 32rpx;
  color: #ccc;
  width: 50%;
  height: 80rpx;
  line-height: 80rpx;
  text-align: center;
}

.sure {
  background: linear-gradient(90deg, rgba(255, 90, 144, 1), rgba(255, 44, 86, 1));
  font-size: 32rpx;
  color: #fff;
  width: 50%;
  height: 80rpx;
  line-height: 80rpx;
  text-align: center;
}

.type {
  padding: 0 30rpx;
}

.type .top {
  font-size: 28rpx;
  font-family: PingFang SC;
  font-weight: bold;
  color: rgba(51, 51, 51, 1);
  margin-top: 40rpx;
}

.type .btn {
  flex-wrap: wrap;
}

.type .btn .item {
  background: rgba(255, 255, 255, 1);
  border: 1rpx solid rgba(204, 204, 204, 1);
  border-radius: 27rpx;
  padding: 10rpx 30rpx;
  font-size: 24rpx;
  font-family: PingFang SC;
  font-weight: 500;
  color: rgba(51, 51, 51, 1);
  margin-right: 40rpx;
  margin-top: 30rpx;
}
/* .type .btn .item:nth-child(5n){
  margin-right: 0;
} */
.type .btn .active {
  color: #ff2c56;
  border: 1rpx solid #ff2c56;
}

.pageFiexed {
  overflow: hidden;
  height: 100vh;
  width: 100vw;
  position: fixed;
  top: 0;
  left: 0;
  bottom: 0;
  right: 0;
}
.tabs{
  margin-top: 120rpx ;
  border:none !important;
}
.van-tab--active {
  color: #ff2c56 !important;
}

.van-tabs__line {
  width: 30rpx !important;
  left: 6% !important;
  background: #ff2c56 !important;


}
.van-tab{
  flex-basis:15% !important;
}
.van-tabs__nav--line{
  padding: 0 30rpx;
  border: none !important;
}
.itembox{
  padding: 0 0rpx 0 30rpx;
  flex-wrap: wrap;
  margin-top: 90rpx;
  box-sizing: border-box;
}
.itembox .item{
  margin-top: 40rpx;
  width:330rpx;
  margin-right: 30rpx;
}
.itembox .item .img{
  width:330rpx;
  height:330rpx;
  background:rgba(245,245,245,1);
  border-radius:20rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  position: relative;
}
.itembox .item .img .over{
  width:120rpx;
  height:50rpx;
  background:rgba(102,102,102,1);
  border-radius:6rpx;
  font-size:28rpx;
  font-family:PingFang SC;
  font-weight:400;
  position:absolute;
  top:50%;
  left:50%;
  transform: translate(-50%,-50%);
  line-height: 50rpx;
  text-align: center;
  color:#FFFFFF;
}
.itembox .item .img image{
  width:330rpx;
  height:330rpx;
  background:rgba(245,245,245,1);
  border-radius:20rpx;
}
.itembox .item .intro{
  font-size:26rpx;
  font-family:PingFang SC;
  font-weight:500;
  color:rgba(17,17,17,1);
  line-height:40rpx;
  margin-top: 10rpx;
}
.itembox .item .price{
  font-size:22rpx;
  font-family:PingFang SC;
  font-weight:bold;
  color:rgba(255,44,86,1);
  /* line-height:40rpx; */
  margin-top: 10rpx;
  align-items: center;
  justify-content: space-between;
}
.itembox .item .price .right{
  font-size:22rpx;
  font-family:PingFang SC;
  font-weight:400;
  color:rgba(153,153,153,1);
}