  font-weight:bold;
  color:rgba(51,51,51,1);
  margin-top: 40rpx;
}
.type .btn{
 
  flex-wrap:wrap;

}
.type .btn .item{
  background:rgba(255,255,255,1);
  border:1rpx solid rgba(204,204,204,1);
