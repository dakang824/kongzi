.loading {
  text-align: center;
  font-size: 28rpx;
  color: #999;
  padding: 30rpx 0;
}
.page .box {
  width: 640rpx;
  background: #fff;
  box-shadow: 0px 0px 50rpx 0px rgba(153, 153, 153, 0.3);
  border-radius: 20rpx;
  margin: 30rpx auto 0;
  padding: 25rpx;
}

.page .box .img {
  width: 150rpx;
  height: 150rpx;
  min-width: 150rpx;
  background: #f5f5f5;
  border-radius: 6rpx;
  display: flex;
  justify-content: center;
  align-items: center;
}

.page .box .img image {
  width: 100%;
  height: 100%;
}

.page .box .flex {
  align-items: center;
  width: 100%;
  padding-bottom: 24rpx;
}

.page .box .flex .rt {
  font-size: 22rpx;
  color: #999;
  width: 75%;
  margin-left: 22rpx;
}

.page .box .flex .rt text {
  font-size: 28rpx;
  color: #111;
  display: inline-block;
  width: 100%;
}

.page .box .flex .rt .m {
  margin: 15rpx 0 12rpx;
}

.page .box .user {
  width: 50rpx;
  height: 50rpx;
  border-radius: 50px;
  margin-right: 17rpx;
}

.page .box .li {
  font-size: 24rpx;
  margin-top: 30rpx;
}

.page .box:last-child {
  margin-bottom: 30rpx;
}

.page .box .li text {
  color: #999;
}

.page .box .btn.active {
  background: #ff2c56;
  color: #fff;
}

.page .box .btn {
  width: 150rpx;
  height: 50rpx;
  border: 1px solid #ff2c56;
  border-radius: 25rpx;
  font-size: 24rpx;
  line-height: 50rpx;
  color: #ff2c56;
  margin: 30rpx auto 10rpx;
  display: block;
}

/* --------未开奖列表------ */
.topImg {
  width:100%;
  height: 200rpx !important;
}
.fixed{
  width: 100%;
  position: fixed;
  top:0rpx;
  left:0rpx;
  height: 200rpx;
  z-index: 11;
  background: #fff;
}
.time{
  position: relative;
}
.time .b {
  position: absolute;
  top: 80rpx;
  left: 60rpx;
  color: #fff;
}

.time .b text {
  font-size: 28rpx;
  font-weight: bold;
}
.time .b .t {
  display: inline-block;
  margin-left: 20rpx;
}
.time  .b .t>text {
  width: 60rpx;
  height: 60rpx;
  background: #fff;
  border-radius: 6rpx;
  display: inline-block;
  text-align: center;
  line-height: 60rpx;
  margin-top: 25rpx;
}

.time  .b .t text text {
  font-size: 28rpx;
  font-weight: bold;
  color: #fff;
  background: linear-gradient(120deg, rgba(48, 175, 255, 1) 0%, rgba(51, 53, 232, 1) 100%);
  -webkit-background-clip: text;
  -webkit-text-fill-color: transparent;
}
.time .tip{
  width:750rpx;
  height:60rpx;
  background:rgba(255,255,255,0.2);
  position: absolute;
  top: 0rpx;
  left: 0rpx;
  font-size:26rpx;
  font-family:PingFang SC;
  font-weight:500;
  color:rgba(252,255,0,1);
  line-height:44rpx;
  display: flex;
  justify-content: center;
  align-items:center;
}
.time .tip image{
  width:24rpx;
  height: 28rpx;
  margin-right: 15rpx;
}
.list {
  padding: 30rpx;
  display: flex;
  flex-wrap: wrap;
  justify-content: space-between;
}

.list .li {
  width: 330rpx;
  height: 500rpx;
  background: #fff;
  box-shadow: 0px 0px 50rpx 0px rgba(153, 153, 153, 0.3);
  border-radius: 20rpx;
  overflow: hidden;
  margin-bottom: 25rpx;
}

.list .li .img {
  height: 320rpx;
  background: #f5f5f5;
  display: flex;
  justify-content: center;
  align-items: center;
  position: relative;
}

.list .li .img .t {
  width: 180rpx;
  height: 180rpx;
  background: rgba(0, 0, 0, 1);
  opacity: 0.25;
  border-radius: 50%;
  position: absolute;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  margin: auto;
  z-index: 9;
  text-align: center;
  line-height: 180rpx;
  font-size: 80rpx;
  color: #fff;
  font-weight:200;
}

.list .li image {
  width: 80%;
}
.list .li .foot {
  padding: 30rpx 20rpx;
}

.list .li .foot .t {
  font-size: 24rpx;
}

.list .li .foot .m {
  font-size: 22rpx;
  color: #666;
  margin: 17rpx 0 15rpx;
}

.list .li .foot .between {
  font-size: 22rpx;
  color: #999;
}

.list .li .foot .between .btn {
  width: 100rpx;
  height: 35rpx;
  background: #ffe9ee;
  border-radius: 50px;
  font-size: 20rpx;
  line-height: 35rpx;
  color: #ff2c56;
}
.nodatabox .img{
  text-align:center;
  margin-top:200rpx;
}
.nodatabox .tip{
  text-align: center;
  font-size:28rpx;
  font-family:PingFang SC;
  font-weight:400;
  color:rgba(51,51,51,1);
  line-height:54rpx;
  margin-top:49rpx;
}
.nomore{
  font-size:28rpx;
  font-family:PingFang SC;
  font-weight:400;
  color:rgba(51,51,51,1);
  line-height:44rpx;
  text-align: center;
}
.big_img{
  position: relative;
}
.big_img .big{
  position: absolute;
  top: 0rpx;
  left: 0rpx;
}
.list .li .foot .between .btn.green{
  color:rgba(62, 156, 7, 0.884);
}

