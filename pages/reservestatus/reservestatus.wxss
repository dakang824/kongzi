}

.schooladdress, .child{
  font-size:22rpx;
  font-family:PingFang-SC-Regular;
  font-weight:400;
  color:rgba(153,153,153,1);
  line-height:40rpx;
  white-space:normal; 
  word-break:break-all;
}

.schooladdress{
  margin-top: 40rpx;
}


.booked{
  position: relative;
  font-size:22rpx;
  font-family:PingFang-SC-Regular;
