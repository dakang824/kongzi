.position {
  position: fixed;
  left: 0;
  top: 0;
  z-index: 100;
}
.booked .op {
  border-top: 1px solid #f3f3f3;
  display: flex;
  justify-content: flex-end;
  padding: 20rpx 0 0 0;
}
.booked .op button {
  width: 50%;
  height: 44rpx;
  background: rgba(255, 255, 255, 1);
  font-size: 28rpx;
  color: rgba(77, 165, 252, 1);
  line-height: 44rpx;
  margin: 0;
  display: flex;
  justify-content: center;
  align-items: center;
  border-radius: 0;
}
.booked .op button:first-child {
  border-right: 1px solid #eee;
}

.booked .op button:first-child {
  margin-right: 20rpx;
}
.booked .circle {
  width: 28rpx;
  height: 28rpx;
  margin-right: 14rpx;
}
page{
  background: rgba(250, 250, 250,1);
}
.content{
  margin-top: 70rpx;
}

.headTopNav{
  height: 0;
}
.van-tabs__wrap, .van-tabs__nav, .van-tab {
  background: #FA2335 !important;
  color: #fff !important;
}
.van-tabs__wrap{
  position: fixed!important;
  top: 0!important;
}
.noData{
  text-align: center;
  font-size: 28rpx;
  color: #999;
  margin-top: 200rpx;
}
.swiper-tab{
  width: 100%;
  text-align: center;
  height: 81rpx;
  line-height: 81rpx;
  font-weight: bold;
  background: #4da5fc;
  display: flex;
  justify-content: space-around;
}
.swiper-tab-item{
  display: inline-block;
  font-size:27rpx;
  font-family:PingFang-SC-Bold;
  font-weight:400;
  color:#fff;
}
.utils .swiper_box{
  margin-bottom: 24rpx;
}
.swiper_box{
  width:630rpx;
  background:rgba(255,255,255,1);
  box-shadow:0rpx 1rpx 50rpx 0rpx rgba(153,153,153,0.3);
  border-radius:20rpx;
  margin: 24rpx auto 0;
  padding: 32rpx 30rpx 25rpx 34rpx;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  position: relative;
}

.swiper_box::after{
  content: '';
  position: absolute;
  left: 30rpx;
  top: 79rpx;
  width: 630rpx;
  height: 1rpx;
  background:rgba(238,238,238,1);
}

.s_b_t_left{
  font-size:24rpx;
  font-family:PingFang-SC-Regular;
  font-weight:400;
  color:rgba(153,153,153,1);
  float: left;
  line-height: 24rpx;
}

.s_b_t_right{
  font-size:24rpx;
  font-family:PingFang-SC-Regular;
  font-weight:400;
  color:rgba(77,165,252,1);
  float: right;
  line-height: 24rpx;
}

.s_b_b{
  margin: 51rpx 0 0 0;
  display: flex;
}

.s_b_b image{
  max-width: 200rpx;
  min-width: 200rpx;
  height: 160rpx;
  float: left;
  border-radius: 10rpx;
}

.s_b_b_right{
  float: left;
  padding-left: 30rpx;
  max-width:64%;
}

.schoolname{
  font-size:24rpx;
  font-weight:bold;
